{
  "video_processing": {
    "fps_extraction": 2.0,
    "max_frames_per_video": 1000,
    "resize_frames": true,
    "target_size": [640, 640],
    "supported_formats": [".mp4", ".avi", ".mov", ".mkv", ".wmv", ".flv", ".webm"]
  },
  "annotation": {
    "confidence_threshold": 0.25,
    "create_empty_annotations": true,
    "use_ensemble_models": true,
    "ensemble_models": ["yolo11n.pt", "yolo11s.pt", "yolov8n.pt"],
    "max_models_in_ensemble": 3,
    "iou_threshold": 0.6,
    "tta_enabled": false
  },
  "dataset": {
    "train_ratio": 0.7,
    "val_ratio": 0.2,
    "test_ratio": 0.1,
    "class_names": [
      "person",
      "chair", 
      "dining_table",
      "cup",
      "bowl",
      "bottle",
      "wine_glass",
      "fork",
      "knife",
      "spoon",
      "plate",
      "food",
      "phone",
      "book",
      "laptop"
    ],
    "min_images_per_split": 10
  },
  "quality_control": {
    "min_detection_size": 0.01,
    "max_detection_size": 0.8,
    "aspect_ratio_range": [0.2, 5.0],
    "duplicate_removal_enabled": true
  },
  "output": {
    "save_visualizations": false,
    "create_reports": true,
    "log_level": "INFO",
    "cleanup_temp_files": true
  },
  "performance": {
    "batch_size": 1,
    "num_workers": 1,
    "use_gpu": true,
    "memory_limit_gb": 8
  }
}